<resources>

  <style name="skipButtonStyle" parent="android:Widget.Button">
    <item name="android:fontFamily">@font/avenir</item>
  </style>


<style name="AppTheme.NoActionBar">
  <item name="windowActionBar">false</item>
  <item name="windowNoTitle">true</item>
</style>

<style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

<style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

  <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
    <item name="boxBackgroundColor">?attr/bgTextEdit</item>
    <item name="boxStrokeColor">@color/colorPrimary</item>
  </style>

  <style name="CaminosTextEditStyle" parent="android:Widget.EditText">
    <item name="android:fontFamily">@font/avenir</item>
    <item name="android:background">@drawable/shapefortextedits</item>
  </style>



</resources>
